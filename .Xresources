! general config
URxvt*urgentOnBell: true
URxvt*cursorBlink: false
URxvt*termName: rxvt-unicode-256color
URxvt*scrollBar: false
URxvt*savelines: 10000
URxvt*iso14755: false
URxvt*depth: 32

! font config
!URxvt*letterSpace: -1
URxvt*font: xft:Go Mono:style=regular:size=12,xft:FontAwesome5Free:size=10
URxvt*allow_bold: false

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! colors
#include "/home/newtant/dotfiles/colors/colors"
#include "/home/newtant/dotfiles/rofi.conf"
